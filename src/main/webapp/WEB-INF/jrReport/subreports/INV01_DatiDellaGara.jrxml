	<parameter name="NUM_ULT_PRESTA" class="java.lang.Integer" isForPrompting="false"/>
	<parameter name="IDSTAMPA" class="java.lang.String"/>
	<queryString>
		<![CDATA[SELECT
GARE.CODCIG AS codice_cig,
T1.TAB1DESC AS tipo_appalto,
GARE.NOT_GAR AS oggetto,
T2.TAB1DESC AS tipo_procedura,
TORN.GARTEL AS telematica,
GARE.CRITLICG AS cod_criterio_aggiudicazione,
T3.TAB1DESC AS criterio_aggiudicazione,
GARE.DETLICG AS codice_determinato_mediante,
T4.TAB1DESC AS determinato_mediante,
GARE.IMPAPP AS importo_base_gara,
GARE.IMPNRL AS importo_no_ribasso,
GARE.IMPSIC AS importo_sicurezza,
T8.TAB1DESC AS offerta_espressa_mediante,
TORN.OFFAUM AS ammesse_offerte_in_aumento,
UFFINT.NOMEIN AS staz_app,
UFFINT.EMAIIN AS email_staz_app,
TECNI.NOMTEC AS rup,
TORN.DTEOFF AS data_scad_presentazione_off,
TORN.OTEOFF AS ora_scad_presentazione_off,
TORN.VALOFF AS num_giorni_validita_off,
TORN.DTERMRICHCPO AS data_ter_rich_chiarimenti,
TORN.DTERMRISPCPO AS data_ter_risp_chiarimenti
FROM GARE
JOIN TORN ON TORN.CODGAR = GARE.CODGAR1
LEFT JOIN TAB1 T1 ON T1.TAB1TIP = TORN.TIPGEN AND T1.TAB1COD = 'A1007'
LEFT JOIN TAB1 T2 ON T2.TAB1TIP = GARE.TIPGARG AND T2.TAB1COD = 'A2044'
LEFT JOIN TAB1 T3 ON T3.TAB1TIP = GARE.CRITLICG AND T3.TAB1COD = 'A2081'
LEFT JOIN TAB1 T4 ON T4.TAB1TIP = GARE.DETLICG AND T4.TAB1COD = 'A2082'
LEFT JOIN UFFINT ON UFFINT.CODEIN = TORN.CENINT
LEFT JOIN TECNI ON TECNI.CODTEC = TORN.CODRUP
LEFT JOIN TAB1 T8 ON T8.TAB1TIP = GARE.RIBCAL AND T8.TAB1COD = 'A1053'
LEFT JOIN V_GARE_CATEGORIE C1 ON C1.NGARA = GARE.NGARA AND C1.ISPREV = '1' AND C1.ISCATGARA = '1'
WHERE GARE.NGARA = $P{NUMERO_GARA}]]>
	</queryString>
