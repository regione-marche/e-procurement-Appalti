	<subDataset name="punto_istruttore" uuid="c873dff3-488c-4626-a75c-3ed2ab7ccfd3">
		<parameter name="NUMERO_GARA" class="java.lang.String"/>
		<queryString>
			<![CDATA[SELECT
USRSYS.SYSUTE AS punto_istruttore
FROM GARE
JOIN G_PERMESSI ON GARE.CODGAR1 = G_PERMESSI.CODGAR AND (G_PERMESSI.MERUOLO = 2 OR G_PERMESSI.MERUOLO = 3) AND G_PERMESSI.AUTORI = 1
JOIN USRSYS ON USRSYS.SYSCON = G_PERMESSI.SYSCON
WHERE GARE.NGARA = $P{NUMERO_GARA}]]>
		</queryString>
		<field name="PUNTO_ISTRUTTORE" class="java.lang.String"/>
	</subDataset>
	<subDataset name="prestazione_principale" uuid="0e71613f-d030-4e36-b72c-982fe393fe08">
		<parameter name="NUMERO_GARA" class="java.lang.String"/>
		<queryString>
			<![CDATA[SELECT
C1.DESCAT AS prestazione_principale
FROM GARE
JOIN V_GARE_CATEGORIE C1 ON C1.NGARA = GARE.NGARA AND C1.ISPREV = '1' AND C1.ISCATGARA = '1'
WHERE GARE.NGARA = $P{NUMERO_GARA}]]>
		</queryString>
		<field name="PRESTAZIONE_PRINCIPALE" class="java.lang.String"/>
	</subDataset>
	<subDataset name="ulteriori_prestazioni" uuid="15689802-c0f3-4e93-afd9-3ba19dba3e2b">
		<parameter name="NUMERO_GARA" class="java.lang.String"/>
		<queryString>
			<![CDATA[SELECT
C2.DESCAT AS ulteriore_prestazione
FROM GARE
JOIN V_GARE_CATEGORIE C2 ON C2.NGARA = GARE.NGARA AND C2.ISPREV = '2' AND C2.ISCATGARA = '1'
WHERE GARE.NGARA = $P{NUMERO_GARA}]]>
		</queryString>
		<field name="ULTERIORE_PRESTAZIONE" class="java.lang.String"/>
	</subDataset>
	<parameter name="SUBREPORT_DIR" class="java.lang.String"/>
	<parameter name="NUMERO_GARA" class="java.lang.String"/>
	<parameter name="NUM_PUNTI_ISTRU" class="java.lang.Integer" isForPrompting="false"/>
